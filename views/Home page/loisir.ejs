                        <img class="f-img img-fluid mx-auto" src="uploads/<%= loisir[i].path%>" alt="" />
                    </div>
                    <div class="details">
                        <div class="d-flex justify-content-between mb-20">
                            <p class="name" style="font-family: 'Almarai', sans-serif;"><%= loisir[i].name %>
                            </p>
                            <p class="value">$150</p>
                        </div>
                        <a href="#">
                            <h4><%= loisir[i].dsc %></h4>
                        </a>
